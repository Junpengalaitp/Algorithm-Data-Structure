                        },
                        "children": [
                            {
                                "data": {
                                    "id": "c9bff209ss80",
                                    "created": 1613526528072,
                                    "text": "剑指offer 51. 数组中的逆序对"
                                },
                                "children": [
                                    {
                                        "data": {
                                            "id": "c9bffqs9t200",
