                            "layout": null
                        },
                        "children": [
                            {
                                "data": {
                                    "id": "c7nxvd69gy80",
                                    "created": 1607484988783,
                                    "text": "LC 83. Remove Duplicates from Sorted List",
                                    "layout": null
                                },
                                "children": [
                                    {
                                        "data": {
                                            "id": "c7nzydw542w0",
                                            "created": 1607490867661,
                                            "text": "use extra space, a hash set",
                                            "layout": null
                                        },
                                        "children": []
                                    },
                                    {
                                        "data": {
                                            "id": "c7nzy01cbc00",
                                            "created": 1607490837500,
                                            "text": "sentinel head + two pointers",
                                            "layout": null
                                        },
                                        "children": []
                                    }
                                ]
                            },
                            {
                                "data": {
                                    "id": "c7nxvifb3hc0",
                                    "created": 1607485000214,
                                    "text": "LC 82. Remove Duplicates from Sorted List II",
                                    "layout": null
                                },
                                "children": [
                                    {
                                        "data": {
                                            "id": "c7nzy01cbc00",
                                            "created": 1607490837500,
                                            "text": "sentinel head + two pointers",
                                            "layout": null
                                        },
                                        "children": [
                                            {
                                                "data": {
                                                    "id": "c7o08np1grk0",
                                                    "created": 1607491672641,
                                                    "text": "use an anchor pointer and an explore pointer, when their value are equal, skip those node.",
                                                    "layout": null
                                                },
                                                "children": []
                                            }
                                        ]
                                    }
                                ]
                            },
                            {
                                "data": {
                                    "id": "c7olbuf0euw0",
                                    "created": 1607551165672,
                                    "text": "LC 92. Reverse LinkedList II",
                                    "layout": null
                                },
                                "children": [
                                    {
                                        "data": {
                                            "id": "c9fksirslk80",
                                            "created": 1613947928675,
                                            "text": "use a new sentinel node and point next for head"
                                        },
                                        "children": []
                                    },
                                    {
                                        "data": {
                                            "id": "c9fkt87ekzc0",
                                            "created": 1613947984038,
                                            "text": "use a headTail pointer for pointing to the last node of first part, this is for connecting the reversed head at the end"
                                        },
                                        "children": []
                                    },
                                    {
                                        "data": {
                                            "id": "c9fkurovhxs0",
                                            "created": 1613948104818,
                                            "text": "use a count to get the reversed part, start with 1"
                                        },
                                        "children": []
                                    },
                                    {
                                        "data": {
                                            "id": "c9fkvbl3dy80",
                                            "created": 1613948148125,
                                            "text": "while count is less than left, move the head and headTail to its next, so at the end of the loop, the head is stopped at the beginning of the reversing part, and the headTail is stopped at its prev node"
                                        },
                                        "children": []
                                    },
                                    {
                                        "data": {
                                            "id": "c9fkx1zc9lk0",
                                            "created": 1613948283947,
                                            "text": "use a new pointer for the tail. while count is less than right, move tail to its next, so at the end of the loop, the tail is stopped at the last node of the reversing part"
                                        },
                                        "children": []
                                    },
                                    {
                                        "data": {
                                            "id": "c9fkyj88t8o0",
                                            "created": 1613948399854,
                                            "text": "start the reverse linked list process, but point prev node to the tail next, and disconnect the tail node with its next"
                                        },
                                        "children": []
                                    },
                                    {
                                        "data": {
                                            "id": "c9fl0423c7k0",
                                            "created": 1613948523559,
                                            "text": "because we need connect the reversed part to head and tail, the end condition for reverse linked list is head.next != null, and need to connect head.next to prev at the end of while loop"
                                        },
                                        "children": []
                                    },
                                    {
                                        "data": {
                                            "id": "c9fl1fm0iw80",
                                            "created": 1613948627072,
                                            "text": "connect headTail to reversed head and return the sentinel.next"
                                        },
                                        "children": []
                                    }
                                ]
                            },
                            {
                                "data": {
                                    "id": "c7pp7wczco00",
                                    "created": 1607663700843,
                                    "text": "LC 234. Palindrome Linked List",
                                    "expandState": "expand"
                                },
                                "children": [
                                    {
                                        "data": {
                                            "id": "c7pp7xmwhio0",
                                            "created": 1607663703619,
                                            "text": "No space: get the mid node, reverse the mid node, compare head and reversed head one by one"
                                        },
                                        "children": []
                                    }
                                ]
                            },
                            {
                                "data": {
                                    "id": "c7pidd0ew800",
                                    "created": 1607644381140,
                                    "text": "LC 143. Reorder List",
                                    "font-weight": "bold"
                                },
                                "children": [
                                    {
                                        "data": {
                                            "id": "c7pigubpqxs0",
                                            "created": 1607644653921,
                                            "text": "With space: use a array to store nodes"
                                        },
                                        "children": []
                                    },
                                    {
                                        "data": {
                                            "id": "c7pih44rp8g0",
                                            "created": 1607644675269,
                                            "text": "No space: spilit the array from mid + 1, reverse the right part, then merge left and right"
                                        },
                                        "children": []
                                    }
                                ]
                            },
                            {
                                "data": {
                                    "id": "c9j77c6j8vs0",
                                    "created": 1614315834079,
                                    "text": "LC 2. Add Two Numbers"
                                },
                                "children": [
                                    {
                                        "data": {
                                            "id": "c9j77n6hawg0",
                                            "created": 1614315858020,
                                            "text": "create a head, a sentinel node point to head, and a carry on value 0"
                                        },
                                        "children": []
                                    },
                                    {
                                        "data": {
                                            "id": "c9j784m15jc0",
                                            "created": 1614315895966,
                                            "text": "while one of the node is not null or carry is not 0"
                                        },
                                        "children": [
                                            {
                                                "data": {
                                                    "id": "c9j78trcwuo0",
                                                    "created": 1614315950708,
                                                    "text": "sum the carry on and nodes value, and move nodes to its next"
                                                },
                                                "children": []
                                            },
                                            {
                                                "data": {
                                                    "id": "c9j796axf880",
                                                    "created": 1614315978012,
                                                    "text": "if the sum >= 10, sum -= 10 and carry become 1"
                                                },
                                                "children": []
                                            },
                                            {
                                                "data": {
                                                    "id": "c9j79hhnu1s0",
                                                    "created": 1614316002364,
                                                    "text": "else the carry is 0"
                                                },
                                                "children": []
                                            },
                                            {
                                                "data": {
                                                    "id": "c9j79lq4xuo0",
                                                    "created": 1614316011584,
                                                    "text": "create a new node with sum value as head.next"
                                                },
                                                "children": []
                                            },
                                            {
                                                "data": {
                                                    "id": "c9j79wxzd340",
                                                    "created": 1614316036003,
                                                    "text": "return sentinel.next"
                                                },
                                                "children": []
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
