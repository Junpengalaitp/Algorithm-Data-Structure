                                ]
                            },
                            {
                                "data": {
                                    "id": "c7p3nwb5tao0",
                                    "created": 1607602890141,
                                    "text": "142. Linked List Cycle II"
                                },
                                "children": [
                                    {
                                        "data": {
                                            "id": "c7p3o6f5s2o0",
                                            "created": 1607602912151,
                                            "text": "With space: use a hash set"
                                        },
                                        "children": []
                                    },
                                    {
                                        "data": {
                                            "id": "c7p3ocz1khs0",
                                            "created": 1607602926414,
                                            "text": "No space: slow and fast pointer, when they are met, poiner one of them to the head, advance one step both until they meet, the meet point is the loop start"
                                        },
                                        "children": []
                                    }
                                ]
                            }
                        ]
